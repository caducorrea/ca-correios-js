{
    "parser": "babel-eslint",
    "extends": "airbnb",
    "env": {
        "mocha": true
    },
    "rules": {
        "prefer-destructuring": [
            "error",
            {
                "array": true,
                "object": true
            },
            {
                "enforceForRenamedProperties": false
            }
        ]
    }
}